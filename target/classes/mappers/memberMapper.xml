<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.spring.study.mapper.MemberMapper">
	<select id="login" resultType="com.spring.study.domain.MemberDTO">
		SELECT *
		FROM tbl_member
		WHERE userid = #{userid}
		AND userpw = #{userpw}
	</select>
	
</mapper>







